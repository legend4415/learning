import java.util.Scanner;
public class TestOdometer {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner input = new Scanner(System.in);
		
		Odometer car1 = new Odometer ();
		// ask user for mpg
		System.out.println("what is your fuel effiency");
		double num1 = input.nextDouble();
		car1.setFuelEffiency(num1);
		// ask user their trip;
		System.out.println("how many miles you driven");
		int num = input.nextInt();
		car1.setMiles(num);
		car1.toString();
		input.close();	

	}

}
